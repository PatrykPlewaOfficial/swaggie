{
  "name": "swaggie",
  "version": "0.5.1",
  "description": "Generate ES6 or TypeScript service integration code from an OpenAPI spec",
  "author": {
    "name": "Piotr Dabrowski",
    "url": "https://github.com/yhnavein"
  },
  "license": "MIT",
  "homepage": "https://github.com/yhnavein/swaggie",
  "repository": {
    "type": "git",
    "url": "https://github.com/yhnavein/swaggie.git"
  },
  "bugs": {
    "url": "https://github.com/yhnavein/swaggie/issues"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "dist/index.js",
  "bin": {
    "swaggie": "dist/cli.js"
  },
  "scripts": {
    "prebuild": "npm run init",
    "build": "tsc",
    "test": "jest --ci",
    "test:watch": "jest --watch",
    "predev": "npm run init",
    "dev": "tsc -w",
    "init": "npm run clean",
    "clean": "rimraf ./dist"
  },
  "files": [
    "dist",
    "templates"
  ],
  "keywords": [
    "swagger",
    "swagger 2.0",
    "openapi",
    "rest",
    "service",
    "typescript",
    "codegen"
  ],
  "dependencies": {
    "chalk": "3.0.0",
    "commander": "6.0.0",
    "ejs": "3.1.3",
    "got": "10.7.0",
    "js-yaml": "3.14.0",
    "lodash": "^4.17.19",
    "mkdirp": "^1.0.3"
  },
  "devDependencies": {
    "@types/ejs": "3.0.4",
    "@types/got": "9.6.11",
    "@types/jest": "26.0.9",
    "@types/js-yaml": "3.12.5",
    "@types/lodash": "4.14.159",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "14.0.27",
    "jest": "26.2.2",
    "rimraf": "3.0.2",
    "ts-jest": "26.1.4",
    "tslint": "6.1.3",
    "typescript": "3.9.7"
  },
  "jest": {
    "testRegex": "\\.(test|spec)\\.tsx?$",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "watchPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ]
  }
}
