{
  "name": "swaggie",
  "version": "0.6.9",
  "description": "Generate ES6 or TypeScript service integration code from an OpenAPI spec",
  "author": {
    "name": "Piotr Dabrowski",
    "url": "https://github.com/yhnavein"
  },
  "license": "MIT",
  "homepage": "https://github.com/yhnavein/swaggie",
  "repository": {
    "type": "git",
    "url": "https://github.com/yhnavein/swaggie.git"
  },
  "bugs": {
    "url": "https://github.com/yhnavein/swaggie/issues"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "main": "dist/index.js",
  "bin": {
    "swaggie": "dist/cli.js"
  },
  "scripts": {
    "build": "sucrase ./src -d ./dist --transforms typescript,imports && npm run rm-tests",
    "rm-tests": "find dist/ -name '*.spec.js' -type f -delete",
    "test": "mocha 'src/**/*.spec.ts'"
  },
  "files": [
    "dist",
    "templates"
  ],
  "keywords": [
    "swagger",
    "swagger 2.0",
    "openapi",
    "rest",
    "service",
    "typescript",
    "codegen"
  ],
  "dependencies": {
    "commander": "^8.3.0",
    "eta": "^1.12.3",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.3",
    "nanocolors": "^0.2.0",
    "node-fetch": "^2.6.3"
  },
  "devDependencies": {
    "@types/chai": "4.3.0",
    "@types/js-yaml": "4.0.5",
    "@types/lodash": "4.14.178",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "9.0.0",
    "@types/node-fetch": "2.5.3",
    "chai": "4.3.4",
    "mocha": "9.1.3",
    "sucrase": "3.20.3",
    "typescript": "4.5.4"
  }
}
